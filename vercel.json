{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/routes/$1.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/routes/googleRoute.js"
      },
      {
        "src": "/auth/google/callback",
        "dest": "/passport.js"
      }
    ]
  }